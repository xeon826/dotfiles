{
  "mcpServers": {
    "serena": {
      "command": "/usr/bin/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ]
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "/usr/bin/uvx"
    },
    "filesystem": {
      "args": ["-y", "mcp-server-filesystem", "${workspaceFolder}"],
      "command": "/usr/bin/npx",
      "env": {
        "USER_HOME": "${userHome}",
        "WORKSPACE_PATH": "${workspaceFolder}"
      }
    },
    "github": {
      "autoApprove": [],
      "disabled": false,
      "disabled_prompts": [],
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "joshua-tree-slack": {
      "command": "/usr/bin/npx",
      "args": [
        "-y",
        "@lamemind/mcp-server-multiverse@latest",
        "/home/dan/.config/mcphub/joshua-tree-slack-mcp.json"
      ],
      "env": {
        "SLACK_MCP_XOXP_TOKEN": "${JOSHUA_TREE_SLACK_MCP_XOXP_TOKEN}"
      }
    },
    "suno-slack": {
      "command": "/usr/bin/npx",
      "args": [
        "-y",
        "@lamemind/mcp-server-multiverse@latest",
        "/home/dan/.config/mcphub/suno-slack-mcp.json"
      ]
    },
    "fraction-slack": {
      "command": "/usr/bin/npx",
      "args": [
        "-y",
        "@lamemind/mcp-server-multiverse@latest",
        "/home/dan/.config/mcphub/fraction-slack-mcp.json"
      ]
    }
  }
}
