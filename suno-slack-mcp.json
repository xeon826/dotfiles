{
  "serverName": "SunoSlack",
  "functionsPrefix": "suno-slack",
  "servers": [
    {
      "command": "/usr/bin/envmcp",
      "args": [
        "npx",
        "-y",
        "slack-mcp-server@latest",
        "--transport",
        "stdio"
      ],
      "env": {
        "SLACK_MCP_ADD_MESSAGE_TOOL": "true",
        "SLACK_MCP_LOG_LEVEL": "info"
      }
    }
  ]
}
